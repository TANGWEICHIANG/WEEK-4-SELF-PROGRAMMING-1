package SelfProgramming;

public class Boolean {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n1 = 10;  
        int n2 = 20;  
          
    boolean b1 = true;  
    boolean b2 = false;  
  
if(n1<n2)  
{  
    System.out.println(b1);  
}  
else  
{  
    System.out.println(b2);  
}  

if(n1>n2)  
{  
    System.out.println(b1);  
}  
else  
{  
    System.out.println(b2);  
}  
	}

}
